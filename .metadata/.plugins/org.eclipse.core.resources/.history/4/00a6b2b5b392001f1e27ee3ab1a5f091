public class Pacman {
    private int x, y; // 위치
    private int speed;

    public Pacman(int startX, int startY) {
        this.x = startX;
        this.y = startY;
        this.speed = 5; // 속도 설정
    }

    public void move(int deltaX, int deltaY) {
        x += deltaX * speed;
        y += deltaY * speed;
    }

    public int getX() {
        return x;
    }

    public int getY() {
        return y;
    }
}
